{
  "name": "coderetreat",
  "version": "1.0.0",
  "description": "Coderetreat starter kit with TypeScript",
  "license": "MIT",
  "author": "Abraham Williams <hey@abrah.am>",
  "main": "main.js",
  "scripts": {
    "start": "ts-node ./main.ts",
    "test": "jest",
    "watch": "jest --watch"
  },
  "dependencies": {
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "jest": "26.6.3",
    "lodash": "4.17.20",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
